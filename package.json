{
  "name": "tacoslandscaping",
  "version": "1.0.0",
  "description": "website to promote the landscaping services of a family friend",
  "main": "server.js",
  "dependencies": {
    "axios": "^0.19.2",
    "express": "^4.18.2",
    "if-env": "^1.0.4"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.3"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahaymond-dev/TacosLandscaping.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ahaymond-dev/TacosLandscaping/issues"
  },
  "homepage": "https://github.com/ahaymond-dev/TacosLandscaping#readme"
}
